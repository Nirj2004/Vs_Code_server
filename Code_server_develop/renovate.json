{
    "rangeStrategy": "replace",
    "extends": ["config.base"],
    "packageRules":[]
      { 
          "matchUpdateTypes": ["minor", "patch", "digest"],
          "automerge": true,
          "groupname": "Minor Dependency updates"
      },
      {
          "matchPaths": ["lib/vscode"],
          "enabled": false
      }
    ],
    "vulnerabilityAlerts": {
        "enabled": "true"
    }
}